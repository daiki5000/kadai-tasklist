<h1>タスク一覧</h1>

<ul>
  <% @tasks.each do |task| %>
    <li><%= link_to task.id,task_path(task) %>:<%= task.status %>&gt;<%= task.content %></li>
  <% end %>
</ul>

<%= link_to 'タスクを作成する',new_task_path %>
<div class="text-center">
  <%= link_to '新規登録する',signup_path, class: 'btn btn-lg btn-primary' %>
</div>
